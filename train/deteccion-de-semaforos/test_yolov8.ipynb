        "import os\n",
        "import shutil\n",
        "from roboflow import Roboflow\n",
        "\n",
        "# Instalar librerías si aún no están\n",
        "!pip install roboflow ultralytics --quiet\n",
        "\n",
        "# Descargar datasets desde Roboflow\n",
        ***REMOVED***
        "\n",
        "ds1 = rf.workspace(\"student-esos5\").project(\"traffic-light-v9orl\").version(3).download(\"yolov8\")\n",
        "ds2 = rf.workspace(\"school-knkqy\").project(\"new-bfmc\").version(6).download(\"yolov8\")\n",
        "ds3 = rf.workspace(\"school-knkqy\").project(\"new-bfmc-car\").version(2).download(\"yolov8\")\n",
        "\n",
        "datasets = {\n",
        "    \"semaforo\": ds1.location,\n",
        "    \"senal\": ds2.location,\n",
        "    \"auto\": ds3.location\n",
        "}\n",
        "\n",
        "# Ruta destino\n",
        "unified_path = \"/content/datasets_unificado\"\n",
        "for split in [\"train\", \"val\"]:\n",
        "    os.makedirs(f\"{unified_path}/images/{split}\", exist_ok=True)\n",
        "    os.makedirs(f\"{unified_path}/labels/{split}\", exist_ok=True)\n",
        "\n",
        "# Offset de clases\n",
        "clases = {\n",
        "    \"semaforo\": {\"offset\": 0, \"names\": [\"semaforo\"]},\n",
        "    \"senal\": {\"offset\": 1, \"names\": [\"senal\"]},\n",
        "    \"auto\": {\"offset\": 2, \"names\": [\"auto\"]}\n",
        "}\n",
        "\n",
        "def copiar_y_remapear(nombre_dataset, dataset_path):\n",
        "    offset = clases[nombre_dataset][\"offset\"]\n",
        "    for split in [\"train\", \"val\"]:\n",
        "        img_src = os.path.join(dataset_path, \"images\", split)\n",
        "        lbl_src = os.path.join(dataset_path, \"labels\", split)\n",
        "        img_dst = os.path.join(unified_path, \"images\", split)\n",
        "        lbl_dst = os.path.join(unified_path, \"labels\", split)\n",
        "\n",
        "        if not os.path.exists(img_src) or not os.path.exists(lbl_src):\n",
        "            print(f\"⚠️ {nombre_dataset.upper()}: no se encontró carpeta '{split}' en {dataset_path}\")\n",
        "            continue\n",
        "\n",
        "        for fname in os.listdir(img_src):\n",
        "            shutil.copy(os.path.join(img_src, fname), os.path.join(img_dst, f\"{nombre_dataset}_{fname}\"))\n",
        "\n",
        "        for fname in os.listdir(lbl_src):\n",
        "            new_lines = []\n",
        "            with open(os.path.join(lbl_src, fname), \"r\") as f:\n",
        "                for line in f:\n",
        "                    parts = line.strip().split()\n",
        "                    if parts:\n",
        "                        parts[0] = str(int(parts[0]) + offset)\n",
        "                        new_lines.append(\" \".join(parts))\n",
        "            with open(os.path.join(lbl_dst, f\"{nombre_dataset}_{fname}\"), \"w\") as f:\n",
        "                f.write(\"\\n\".join(new_lines))\n",
        "\n",
        "# Procesar cada dataset\n",
        "for nombre, path in datasets.items():\n",
        "    copiar_y_remapear(nombre, path)\n",
        "\n",
        "# Crear data.yaml\n",
        "data_yaml = f\"\"\"\n",
        "train: {unified_path}/images/train\n",
        "val: {unified_path}/images/val\n",
        "\n",
        "nc: 3\n",
        "names: ['semaforo', 'senal', 'auto']\n",
        "\"\"\"\n",
        "\n",
        "with open(f\"{unified_path}/data.yaml\", \"w\") as f:\n",
        "    f.write(data_yaml)\n",
        "\n",
        "print(\"✅ Dataset unificado creado con éxito en:\", unified_path)\n"
      ],
